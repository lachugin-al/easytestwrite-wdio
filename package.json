{
  "name": "easytestwrite-wdio",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "wdio:android": "wdio run ./wdio.android.conf.ts",
    "wdio:ios": "wdio run ./wdio.ios.conf.ts",
    "wdio": "wdio run ./wdio.shared.conf.ts",
    "allure:serve": "allure serve ./allure-results",
    "allure:generate": "allure generate ./allure-results --clean -o ./allure-report"
  },
  "dependencies": {
    "allure-commandline": "^2.30.0",
    "allure-js-commons": "^3.3.3",
    "appium": "^3.0.1",
    "appium-uiautomator2-driver": "^5.0.0",
    "appium-xcuitest-driver": "^10.0.4"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.14.10",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "@wdio/allure-reporter": "^9.0.0",
    "@wdio/appium-service": "^9.0.0",
    "@wdio/cli": "^9.0.0",
    "@wdio/globals": "^9.17.0",
    "@wdio/local-runner": "^9.0.0",
    "@wdio/mocha-framework": "^9.0.0",
    "@wdio/spec-reporter": "^9.0.0",
    "@wdio/types": "^9.19.1",
    "eslint": "^9.33.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-mocha": "^11.1.0",
    "eslint-plugin-promise": "^7.2.1",
    "expect-webdriverio": "^5.0.3",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.16.0",
    "typescript": "^5.5.4",
    "webdriverio": "^9.0.0"
  }
}
